{

	"luci-app-kucat-config": {
		"description": "Grant UCI access for luci-app-kucat-config",
		"read": {
			"file": {
				"/www/luci-static/kucat/bg/*": [ "list2" ]
				"/www/luci-static/resources/background/*": [ "list" ]
			},
			"ubus": {
				"luci.kucat": [ "avail", "remove", "rename", "remove2", "rename2"  ]
			},
			"uci": [ "kucat" ]
		},
		"write": {
			"file": {
				"/tmp/kucat_background.tmp": [ "write" ]
			},
			"uci": [ "kucat" ]
		}
	}
}
